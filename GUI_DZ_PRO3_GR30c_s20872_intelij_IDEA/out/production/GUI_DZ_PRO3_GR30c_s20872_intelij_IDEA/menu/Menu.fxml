<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane fx:id="menuPane" prefHeight="448.0" prefWidth="348.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.MenuControler">
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
    <top>
        <Pane blendMode="SRC_ATOP" prefHeight="65.0" prefWidth="319.0" style="-fx-background-color: #000000;" BorderPane.alignment="CENTER">
            <children>
                <Text fill="#44ab1f" layoutX="-14.0" layoutY="48.0" stroke="#080711" strokeType="OUTSIDE" strokeWidth="4.0" text="SNAKE" textAlignment="CENTER" wrappingWidth="375.15380859375">
                    <font>
                        <Font name="OCR A Extended" size="46.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="NONE" />
                    </cursor>
                </Text>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </Pane>
    </top>
    <center>
        <GridPane alignment="CENTER" gridLinesVisible="true" prefHeight="290.0" prefWidth="444.0" style="-fx-background-color: #48665b;" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="exitButton" mnemonicParsing="false" prefHeight="121.0" prefWidth="348.0" style="-fx-background-color: #000000;" text="EXIT" textFill="#44ab1f" GridPane.rowIndex="2" onMouseClicked="#exitGame">
                    <font>
                        <Font name="OCR A Extended" size="30.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button fx:id="newGameButton" alignment="CENTER" mnemonicParsing="false"  prefHeight="121.0" prefWidth="348.0" style="-fx-background-color: #000000;" text="NEW GAME" textFill="#44ab1f" wrapText="true" GridPane.valignment="CENTER" onMouseClicked="#startNewGame">
                    <font>
                        <Font name="OCR A Extended" size="30.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button fx:id="highScoresButton" mnemonicParsing="false" prefHeight="120.0" prefWidth="379.0" style="-fx-background-color: #000000;" text="HIGH SCORES" textAlignment="CENTER" textFill="#44ab1f" GridPane.rowIndex="1" onMouseClicked="#openHighScores">
                    <font>
                        <Font name="OCR A Extended" size="30.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </children>
        </GridPane>
    </center>
</BorderPane>
